{
    "manifest_version": 2,
    "name": "Intelligent JP",
    "version": "0.1.1",
    "description":"Simple tool for recognizing handwritten Japanese characters.",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons":
    {
        "16":"img/icon16.png",
        "48":"img/icon48.png",
        "128":"img/icon128.png"
    },
    "browser_action":
    {
        "default_icon":"img/browser_action.png",
        "default_popup":"open_popup.html"
    },
    "background":
    {
        "scripts":["tf.min.js","background.js"]
    }
  }